#: questions_import.admin.inc:20
msgid "Quiz Node"
msgstr "Noeud de type Quiz"

#: questions_import.admin.inc:22
msgid "Select the quiz node under which you want to add questions"
msgstr "Sélectionnez le noeud de type Quiz auquel vous souhaitez ajouter des questions"

#: questions_import.admin.inc:28
msgid "Question type"
msgstr "Type de question"

#: questions_import.admin.inc:30
msgid "Select the quiz question type you wish to upload"
msgstr "Sélectionnez le type de question que vous souhaitez téléverser"

#: questions_import.admin.inc:36
msgid "Import type"
msgstr "Type d'import"

#: questions_import.admin.inc:38
msgid "Select the import type csv, XML"
msgstr "Sélectionnez le type d'import (csv, XML)"

#: questions_import.admin.inc:44
msgid "Field Separator"
msgstr "Séparateur de Champs"

#: questions_import.admin.inc:45
msgid ","
msgstr ","

#: questions_import.admin.inc:46
msgid "Special character used to separator the fields usually , : or ; "
msgstr "Caractère spécial utilisé pour séparer les champs, habituellement , : ou ;"

#: questions_import.admin.inc:54
msgid "Upload"
msgstr "Téléverser"

#: questions_import.admin.inc:56
msgid "Upload the file that has quiz questions"
msgstr "Téléverser le fichier contenant les questions du quiz"

#: questions_import.admin.inc:61
msgid "import"
msgstr "importer"

#: questions_import.admin.inc:94
#: question_types/multichoice/multichoice.info:0
msgid "Multichoice"
msgstr "Choix multiple"

